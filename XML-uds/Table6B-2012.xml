<table>
      <description>HRSA REPORT TABLE 6B 2012</description>
      <rowproviderSet>
        <rowprovider class="com.palantir.kite.provider.BasicRowProvider">
          <param key="sql">  
 	SELECT hci.[BHCMISID]
				  ,'2012' as ry_lngReportingYear
				  ,'01/01/2012' as ReportingPeriodBegin, '12/31/2012' as ReportingPeriodEnd     
				  ,hci.[HealthCenterName]
				  ,'6B' as UDSTableType
				  ,'TABLE 6B-QUALITY OF CARE INDICATORS-' + hci.[HealthCenterName] + '-2012' as  ReportSectionName 				  
			      ,[T6b_L1_Ca]
			      ,[T6b_L2_Ca]
			      ,[T6b_L3_Ca]
			      ,[T6b_L4_Ca]
			      ,[T6b_L5_Ca]
			      ,[T6b_L6_Ca]
			      ,[T6b_L7_Ca]
			      ,[T6b_L7_Cb]
			      ,[T6b_L8_Ca]
			      ,[T6b_L8_Cb]
			      ,[T6b_L9_Ca]
			      ,[T6b_L9_Cb]
			      ,[T6b_L10_Ca]
			      ,[T6b_L10_Cb]
			      ,[T6b_L10_Cc]
			      ,[T6b_L11_Ca]
			      ,[T6b_L11_Cb]
			      ,[T6b_L11_Cc]
				   ,T6b_L12_Ca
					,T6b_L12_Cb
					,T6b_L12_Cc
					,T6b_L13_Ca
					,T6b_L13_Cb
					,T6b_L13_Cc
					,T6b_L14_Ca
					,T6b_L14_Cb
					,T6b_L14_Cc
					,T6b_L15_Ca
					,T6b_L15_Cb
					,T6b_L15_Cc
					,T6b_L16_Ca
					,T6b_L16_Cb
					,T6b_L16_Cc
					,T6b_L17_Ca
					,T6b_L17_Cb
					,T6b_L17_Cc
					,T6b_L18_Ca
					,T6b_L18_Cb
					,T6b_L18_Cc
					,T6b_L19_Ca
					,T6b_L19_Cb
					,T6b_L19_Cc
					 ,hci.[HealthCenterStreetAddress]
				   --,hci.[HealthCenterOtherAddress]
				   ,hci.[HealthCenterCity]
				   ,hci.[HealthCenterState]
				   ,hci.[HealthCenterZIPCode]
				   ,'US' as Country
				     ,hci.GrantNumber
				     , hci.Longitude, hci.Latitude
			 FROM [HRSA_UDS_2012].[dbo].[Table6B], [HRSA_UDS_2012].[dbo].TBLGRANTEEINFOGEOCODED hci
				where [HRSA_UDS_2012].[dbo].[Table6B].[BHCMISID] = hci.[BHCMISID]
          </param>
        </rowprovider>
      </rowproviderSet>
